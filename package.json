{
  "name": "areanow",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "areanow",
    "manifest": "src/manifest.json",
    "main": "areanow.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "dependencies": {
    "skpm": "^1.1.9"
  },
 "repository": {
       "type": "git",
       "url": "https://github.com/norman-woz/areanow.git"
    }
}
