{
  "name": "area-now",
  "version": "3.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "area-now",
    "manifest": "src/manifest.json",
    "main": "areanow.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "dependencies": {
    "skpm": "^1.1.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norman-woz/areanow.git"
  }
}
